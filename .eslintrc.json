{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "no-plusplus": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/destructuring-assignment": "off",
    "prefer-destructuring": "off",
    "max-len": "off",
    "no-use-before-define": "off",
    "no-loop-func": "off",
    "linebreak-style": 0,
    "properties": "never",
    "camelcase": "off",
    "no-param-reassign": "off"
  }
}
